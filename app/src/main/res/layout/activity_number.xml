<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.example.dell.firstproject.NumberActivity">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
                <Button
                    android:layout_marginTop="9dp"
                    android:background="#FFA500"
                    android:id="@+id/button1"
                    android:layout_width="110dp"
                    android:layout_height="100dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="13dp"
                    android:text="1"
                    android:textSize="50sp"
                    android:textStyle="bold"
                    />

        <Button
            android:background="#FFA500"
            android:id="@+id/button2"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button1"
            android:layout_centerHorizontal="true"
            android:text="2"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:background="#FFA500"
            android:id="@+id/button3"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button2"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="12dp"
            android:text="3"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:layout_marginTop="10dp"
            android:background="#FFA500"
            android:id="@+id/button4"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignStart="@id/button1"
            android:layout_below="@id/button1"
            android:text="4"
            android:textSize="50sp"
            android:textStyle="bold"
           />
        <Button
             android:background="#FFA500"
            android:id="@+id/button5"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button4"
            android:layout_centerHorizontal="true"
            android:text="5"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:background="#FFA500"
            android:id="@+id/button6"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignEnd="@id/button3"
            android:layout_alignTop="@id/button5"
            android:text="6"
            android:textSize="50sp"
            android:textStyle="bold"
            />

        <Button
            android:layout_marginTop="10dp"
            android:background="#FFA500"
            android:id="@+id/button7"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignStart="@+id/button4"
            android:layout_below="@+id/button4"
            android:text="7"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:background="#FFA500"
            android:id="@+id/button8"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button7"
            android:layout_centerHorizontal="true"
            android:text="8"
            android:textSize="50sp"
            android:textStyle="bold"
            />

        <Button
            android:background="#FFA500"
            android:id="@+id/button9"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button8"
            android:layout_alignEnd="@+id/button6"
            android:text="9"
            android:textSize="50sp"
            android:textStyle="bold"
            />

        <Button
            android:layout_marginTop="10dp"
            android:background="#FFA500"
            android:id="@+id/button10"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignStart="@+id/button7"
            android:layout_below="@+id/button7"
            android:text="10"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:background="#FFA500"
            android:id="@+id/button11"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button10"
            android:layout_centerHorizontal="true"
            android:text="11"
            android:textSize="50sp"
            android:textStyle="bold"
            />

        <Button
            android:background="#FFA500"
            android:id="@+id/button12"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button11"
            android:layout_alignEnd="@+id/button9"
            android:text="12"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:layout_marginTop="10dp"
            android:id="@+id/button13"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignStart="@id/button10"
            android:layout_below="@id/button10"
            android:text="13"
            android:background="#FFA500"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:background="#FFA500"
            android:id="@+id/button14"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button13"
            android:layout_centerHorizontal="true"
            android:text="14"
            android:textSize="50sp"
            android:textStyle="bold"
            />

        <Button
            android:background="#FFA500"
            android:id="@+id/button15"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button14"
            android:layout_alignEnd="@+id/button12"
            android:text="15"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:layout_marginTop="10dp"
            android:background="#FFA500"
            android:id="@+id/button16"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignStart="@id/button13"
            android:layout_below="@id/button13"
            android:text="16"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:background="#FFA500"
            android:id="@+id/button17"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button16"
            android:layout_centerHorizontal="true"
            android:text="17"
            android:textSize="50sp"
            android:textStyle="bold"
            />

        <Button
            android:background="#FFA500"
            android:id="@+id/button18"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button17"
            android:layout_alignEnd="@+id/button15"
            android:text="18"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:layout_marginTop="10dp"
            android:background="#FFA500"
            android:id="@+id/button19"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignStart="@id/button16"
            android:layout_below="@id/button16"
            android:text="19"
            android:textSize="50sp"
            android:textStyle="bold"
            />
        <Button
            android:background="#FFA500"
            android:id="@+id/button20"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_alignBottom="@id/button19"
            android:layout_centerHorizontal="true"
            android:text="20"
            android:textSize="50sp"
            android:textStyle="bold"
            />

    </RelativeLayout>
</ScrollView>
